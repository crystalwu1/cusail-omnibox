{
  "name": "CUSail Omnibox",
  "description": "Type 'cusail' plus a keyword to link to the corresponding webpage",
  "version": "1.0",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.js"],
      "run_at": "document_end"
    }
  ],
  "omnibox": {
    "keyword": "cusail"
  },
  "permissions": [
    "storage",
    "identity",
    "activeTab",
    "<all_urls>",
    "tabs"
  ],
  "oauth2": {
    "client_id": "596447799269-cbapkqimpn3ftplhoci5lgeevqd7s1v3.apps.googleusercontent.com",
    "scopes": [
      ""
    ]
  },
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "key": "bncchpmlkiilbcadjeocoghgplpndcpp",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "cusail16.png",
      "32": "cusail32.png"
    }
  },
  "icons": {
    "16": "cusail16.png",
    "32": "cusail32.png",
    "48": "cusail48.png",
    "128": "cusail128.png"
  }
}
